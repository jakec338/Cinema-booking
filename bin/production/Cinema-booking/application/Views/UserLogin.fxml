<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="540.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.UserLoginController">
    <children>
        <TextField fx:id="txtName" layoutX="179.0" layoutY="101.0" promptText="Username" />
        <PasswordField fx:id="txtPass" layoutX="179.0" layoutY="162.0" promptText="Password" />
        <Label fx:id="lblStatus" layoutX="179.0" layoutY="42.0" text="Please enter your user login details" textFill="#500778" />
        <Button fx:id="loginBtn" layoutX="179.0" layoutY="226.0" mnemonicParsing="false" onAction="#handleUserLogin" text="Login" />
        <Button fx:id="backBtn" layoutX="40.0" layoutY="42.0" mnemonicParsing="false" onAction="#handleUserLogin" prefHeight="20.0" prefWidth="50.0" text="Back" />
        <Button fx:id="registerBtn" layoutX="40.0" layoutY="101.0" mnemonicParsing="false" onAction="#handleUserLogin" prefHeight="20.0" prefWidth="100.0" text="Register" />
    </children>
</AnchorPane>
