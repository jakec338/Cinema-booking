<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Booking.addFilm.AddFilmSceneController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="28.0" layoutY="24.0" text="Enter Film Details">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <GridPane layoutX="28.0" layoutY="96.0" prefHeight="195.0" prefWidth="287.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="147.0" minWidth="10.0" prefWidth="142.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="134.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="43.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="69.0" minHeight="10.0" prefHeight="49.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="78.0" minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="titleLabel" text="Title" />
                  <Label fx:id="directorLabel" text="Director" GridPane.rowIndex="1" />
                  <TextField fx:id="titleTextField" GridPane.columnIndex="1" />
                  <TextField fx:id="directorTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Description" GridPane.rowIndex="2" />
                  <TextArea fx:id="descriptionTextField" prefHeight="54.0" prefWidth="134.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Separator layoutX="28.0" layoutY="65.0" prefHeight="0.0" prefWidth="325.0" />
            <Button fx:id="submitFilmBtn" layoutX="28.0" layoutY="293.0" mnemonicParsing="false" onAction="#handleSubmit" text="Submit" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
