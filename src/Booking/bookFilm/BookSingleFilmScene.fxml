<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.TextFlow?>

<BorderPane stylesheets="/Booking/Styling.css" styleClass="background" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Booking.bookFilm.BookSingleFilmSceneController">
    <center>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Film Details">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="200.0">
                            <children>
                                <GridPane layoutX="28.0" layoutY="55.0" prefWidth="500.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="250.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="250.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="Title" />
                                        <Label fx:id="titleLabel" text="Label" GridPane.columnIndex="1" />
                                        <Label text="Director" GridPane.rowIndex="1" />
                                        <Label fx:id="directorLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                        <Label text="Description" GridPane.rowIndex="2" />
                                        <TextFlow fx:id="descriptionTextFlow" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                    </children>
                                </GridPane>
                                <AnchorPane fx:id="imgAnchor" layoutX="600.0" layoutY="55.0" prefHeight="202.0" prefWidth="159.0" maxWidth="100">
                                    <ImageView/>
                                </AnchorPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Showing Times">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <FlowPane fx:id="flowPane" layoutX="15.0" layoutY="84.0" prefHeight="200.0" prefWidth="56.0">
                                    <children>
                                        <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" />
                                    </children>
                                </FlowPane>
                                <Rectangle fx:id="seatA2" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="77.0" layoutX="427.0" layoutY="276.0" stroke="BLACK" strokeType="INSIDE" width="83.0" />
                                <FlowPane layoutX="273.0" layoutY="14.0" prefHeight="0.0" prefWidth="92.0">
                                    <children>
                                        <VBox fx:id="seatsVbox" prefHeight="14.0" prefWidth="92.0" />
                                    </children>
                                </FlowPane>
                                <AnchorPane fx:id="seatsAnchorPane" layoutX="236.0" layoutY="51.0" prefHeight="200.0" prefWidth="200.0" />
                                <StackPane fx:id="seatsStackPane" layoutX="427.0" layoutY="76.0" prefHeight="150.0" prefWidth="161.0" />
                                <ComboBox fx:id="dateComboBox" layoutX="14.0" layoutY="38.0" prefWidth="150.0" promptText="Select Date" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>