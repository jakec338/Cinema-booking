<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.layout.GridPane?>
<BorderPane stylesheets="/Booking/Styling.css" styleClass="background" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Booking.login.LoginSceneController">
   <center>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane vgap="20" prefWidth="200" layoutX="500" layoutY="200">
            <Button fx:id="loginBtn" GridPane.rowIndex="3" mnemonicParsing="false" onAction="#Login" text="Login" />
            <Label fx:id="loginLabel" GridPane.rowIndex="0" text="Enter Details" textAlignment="CENTER" />
            <PasswordField fx:id="password" GridPane.rowIndex="2" promptText="Password" />
            <TextField fx:id="username" GridPane.rowIndex="1" promptText="Username" />
            </GridPane>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
